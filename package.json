{
  "name": "hyperliquid-arb-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "test:refinement": "tsx src/refinement/test-init.ts",
    "test:execution": "tsx src/execution/test-execution.ts",
    "start:execution": "tsx src/execution/main.ts",
    "test:e2e": "tsx src/tests/integration/test-e2e-pipeline.ts",
    "test:full-pipeline": "tsx src/tests/test-full-pipeline.ts",
    "start:integration": "tsx src/integration/main.ts",
    "monitor": "tsx src/monitoring/dashboard.ts",
    "lint": "echo 'no lint configured'",
    "dev:8083": "set TS_API_PORT=8083&& ts-node-dev --respawn --transpile-only src/api/server.ts",
    "smoke": "ts-node -T scripts/smoke_eval.ts",
    "feeds": "ts-node src/feeds/index.ts",
    "feeds:dev": "ts-node-dev --respawn --transpile-only src/feeds/index.ts",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "seed:strategy": "tsx scripts/seed_strategy.ts"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ethers": "^5.7.2",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "node-fetch": "^2.7.0",
    "prom-client": "^15.1.3",
    "proper-lockfile": "^4.1.2",
    "tsx": "^3.14.0",
    "viem": "^2.33.3",
    "winston": "^3.17.0",
    "ws": "^8.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/node-fetch": "^2.6.13",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^1.6.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
